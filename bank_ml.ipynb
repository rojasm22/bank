      "source": [
        "**Randomized Search**"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "O6mNw8LPLZ2u",
        "outputId": "9c09f4a8-0da5-4566-efa4-1cc191852bc2"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Fitting 3 folds for each of 10 candidates, totalling 30 fits\n"
          ]
        }
      ],
      "source": [
        "# Set the search budget (number of parameter combinations to try)\n",
        "budget = 10\n",
        "\n",
        "# Define the hyperparameter space for the randomized search\n",
        "param_rand_knn = {\n",
        "    # Number of neighbors to consider (K), sampled randomly from 1 to 100\n",
        "    \"knn__n_neighbors\": randint(1, 101),  # randint gives integer samples\n",
        "\n",
        "    # How neighbors are weighted: uniform or by distance\n",
        "    \"knn__weights\": [\"uniform\", \"distance\"],\n",
        "\n",
        "    # Power parameter for Minkowski distance: 1 = Manhattan, 2 = Euclidean\n",
        "    \"knn__p\": [1, 2],\n",
        "}\n",
        "\n",
        "# Create a RandomizedSearchCV object for KNN\n",
        "knn_rand = RandomizedSearchCV(\n",
        "    estimator=pipe_knn_ord,            # Pipeline with preprocessing and KNN classifier\n",
        "    param_distributions=param_rand_knn,# Hyperparameter space to sample from\n",
        "    n_iter=budget,                     # Number of random combinations to evaluate\n",
        "    scoring=\"accuracy\",                # Metric to evaluate model performance\n",
        "    cv=inner,                          # Cross-validation strategy\n",
        "    n_jobs=-1,                         # Use all available CPU cores\n",
        "    verbose=1,                         # Print progress during training\n",
        "    random_state=100576018             # Ensures reproducible results\n",
        ")\n",
        "\n",
        "# Fit the randomized search on the training data to find the best parameter combination\n",
        "begin_knn_rand = time.perf_counter()\n",
        "knn_rand.fit(X_train, y_train)\n",
        "end_knn_rand = time.perf_counter()\n",
        "time_knn_rand = end_knn_rand - begin_knn_rand"
      ]
    },
