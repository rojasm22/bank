        "print(f\"Ordinal KNN: {accuracy_knn_ord:.4f} vs Fixed KNN: {accuracy_knn_fix:.4f}\")\n",
        "print(f\"Ordinal Tree: {accuracy_tree_ord:.4f} vs Fixed Tree {accuracy_tree_fix:.4f}\")\n",
        "print(f\"Ordinal Logistic Regression: {accuracy_reglog_ord:.4f} vs Fixed RegLog {accuracy_reglog_fix:.4f}\")"
