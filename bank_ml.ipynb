          "text": [
            "Test size 0.20 accuracy: 0.8259\n",
            "Test size 0.33 accuracy: 0.8157\n",
            "Test size 0.50 accuracy: 0.8116\n"
          ]
        }
      ],
      "source": [
        "for i in [0.2,1/3,0.5]:\n",
        "  X_train, X_test, y_train, y_test = train_test_split(x_df, y_df, test_size=i, random_state=100576018)\n",
        "  clf_knn = neighbors.KNeighborsClassifier()\n",
        "\n",
        "  # We create a pipeline that first scales the data and then trains the model\n",
        "  pipe = Pipeline([\n",
        "      ('preprocessor', preprocessor_ord),\n",
        "      ('knn', clf_knn)]\n",
        "  )\n",
        "\n",
        "  # Now, we train it\n",
        "\n",
        "  pipe.fit(X_train, y_train)\n",
        "\n",
        "  y_pred = pipe.predict(X_test)\n",
        "  accuracy_knn = metrics.accuracy_score(y_test, y_pred)\n",
        "  print(f\"Test size {i:.2f} accuracy: {accuracy_knn:.4f}\")"
