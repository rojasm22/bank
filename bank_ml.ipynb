        "\n",
        "# Create a RandomizedSearchCV object for Logistic Regression\n",
        "reglog_rand = RandomizedSearchCV(\n",
        "    estimator=pipe_reglog_fix,       # Pipeline with preprocessing and logistic regression\n",
        "    param_distributions=param_rand_reglog,  # Parameter space to sample from\n",
        "    n_iter=budget,                   # Number of random combinations to evaluate\n",
        "    scoring=\"accuracy\",              # Evaluation metric for model performance\n",
        "    cv=inner,                        # Cross-validation strategy\n",
        "    n_jobs=-1,                       # Use all available CPU cores for parallel processing\n",
        "    verbose=1,                       # Print progress during training\n",
        "    random_state=100576018           # Ensures reproducible random sampling\n",
        ")\n",
        "\n",
        "# Fit the randomized search on the training data to find the best parameters\n",
        "begin_reglog_rand = time.perf_counter()\n",
        "reglog_rand.fit(X_train, y_train)\n",
        "end_reglog_rand = time.perf_counter()\n",
        "time_reglog_rand = end_reglog_rand - begin_reglog_rand\n"
