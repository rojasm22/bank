      "source": [
        "# Define the hyperparameter search space for Optuna optimization of Random Forest\n",
        "param_optuna_forest = {\n",
        "    # Number of trees in the forest. We test 100, 200, 300, 400, 500\n",
        "    \"forest__n_estimators\": IntDistribution(100, 500, step=100),\n",
        "\n",
        "    # Number of features to consider when looking for the best split\n",
        "    # 'sqrt' = square root of total features, 'log2' = logarithm base 2 of total features\n",
        "    \"forest__max_features\": CategoricalDistribution([\"sqrt\", \"log2\"]),\n",
        "\n",
        "    # Minimum number of samples required to split an internal node\n",
        "    \"forest__min_samples_split\": IntDistribution(2, 10, step=1),\n",
        "}\n",
        "\n",
        "# Set the search budget: total number of trials Optuna will run\n",
        "budget = 30  # 30 trials to make it faster\n",
        "\n",
        "# Create an Optuna-based hyperparameter search for the Random Forest pipeline\n",
        "forest_optuna = optuna_integration.OptunaSearchCV(\n",
        "    estimator=pipe_forest,              # Pipeline containing preprocessing and Random Forest classifier\n",
        "    param_distributions=param_optuna_forest,  # Search space for Optuna to explore\n",
        "    scoring='accuracy',                  # Metric to maximize during optimization\n",
        "    n_trials=budget,                     # Total number of trials to execute\n",
        "    cv=inner,                            # Cross-validation strategy\n",
        "    n_jobs=1,                            # Number of parallel jobs (1 = sequential execution)\n",
        "    verbose=1,                           # Display progress and trial results\n",
        "    timeout=600,                         # Maximum search time in seconds (10 minutes)\n",
        "    random_state=100576018,              # Seed for reproducibility\n",
        ")\n"
