        "\n",
        "# Fit (train) the entire pipeline using the training data\n",
        "begin_svm = time.perf_counter()\n",
        "pipe_svm.fit(X_train, y_train)\n",
        "end_svm = time.perf_counter()\n",
        "time_svm = end_svm - begin_svm\n",
        "\n",
        "# Use the trained pipeline to make predictions on the test set\n",
        "y_pred_svm = pipe_svm.predict(X_test)\n",
        "\n",
        "# Calculate the accuracy of the predictions\n",
        "accuracy_svm = metrics.accuracy_score(y_test, y_pred_svm)\n",
        "f1_svm = metrics.f1_score(y_test, y_pred_svm)"
