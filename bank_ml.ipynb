        "# 1. Numerical variables (including engineered features and cleaned 'pdays')\n",
        "# Note: 'pdays' no longer requires special handling of -1 since the preprocessing\n",
        "#       function replaces it with 999.\n",
        "eng_numeric_cols = [\n",
