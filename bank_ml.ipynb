      "source": [
        "## Month \"Fixed Effects\"\n",
        "This transformation allows the model to capture month-specific effects, similar to controlling for fixed effects in panel data models, by creating dummy variables for each month."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "bKFIIj904384"
      },
      "outputs": [],
      "source": [
        "# Definition\n",
        "numeric_fix= ['age', 'balance', 'day', 'duration', 'campaign', 'previous']\n",
        "pdays_col_fix= [\"pdays\"]\n",
        "categorical_fix = ['job', 'marital', 'education', 'default', 'housing', 'loan', 'contact', 'poutcome','month'] ##we add month as a categorical variable\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "##Numerical variables\n",
        "numerical_transformer_fix = Pipeline(steps=[\n",
        "    ('scaler',StandardScaler())\n",
        "])\n",
        "\n",
        "##Categorical variables\n",
        "categorical_transformer_fix = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='most_frequent')),\n",
        "    ('onehot', OneHotEncoder(handle_unknown='ignore',drop=\"first\")) ##drop first to avoid multicolinearity\n",
        "])\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "##Pdays variable\n",
        "pdays_indicator_transformer_fix = Pipeline(steps=[\n",
        "    ('indicator', MissingIndicator(missing_values=-1))\n",
        "])\n",
        "\n",
        "\n",
        "pdays_value_transformer_fix = Pipeline(steps=[\n",
        "    ('scaler', StandardScaler())\n",
        "])\n",
        "\n",
        "##General preprocessor\n",
        "preprocessor_fix = ColumnTransformer(\n",
        "    transformers=[\n",
        "        ('num', numerical_transformer_fix, numeric_fix),\n",
        "        ('cat', categorical_transformer_fix, categorical_fix),\n",
        "        ('pdays_contacted', pdays_indicator_transformer_fix, pdays_col_fix),  # MissingIndicator\n",
        "        ('pdays_days', pdays_value_transformer_fix, pdays_col_fix)\n",
        "    ],remainder=\"passthrough\")"
      ]
    },
