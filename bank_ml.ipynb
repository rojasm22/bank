      "source": [
        "# Set the search budget (number of parameter combinations to try)\n",
        "budget = 7\n",
        "\n",
        "# Define the hyperparameter space for the randomized search\n",
        "param_rand_reglog = {\n",
        "    # Regularization strength is sampled from a continuous uniform distribution between 0.01 and 100\n",
        "    \"reglog__C\": uniform(0.01, 100),\n",
        "\n",
        "    # Type of regularization to test (L1 or L2)\n",
        "    \"reglog__penalty\": [\"l2\", \"l1\"],\n",
        "\n",
        "    # Optimization solver supporting both penalties\n",
        "    \"reglog__solver\": [\"liblinear\"],\n",
