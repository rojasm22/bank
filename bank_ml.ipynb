      "outputs": [],
      "source": [
        "# Define the model\n",
        "clf_knn_cy = neighbors.KNeighborsClassifier()\n",
        "\n",
        "# Create a pipeline that first applies preprocessing steps,\n",
        "# and then fits a K-Nearest Neighbors (KNN) classifier\n",
        "pipe_knn_cy = Pipeline([\n",
        "    ('preprocessor', preprocessor_cy),  # Step 1: apply transformations (e.g., scaling, encoding)\n",
        "    ('knn', clf_knn_cy)                 # Step 2: train the KNN classifier\n",
        "])\n",
        "\n",
        "# Fit (train) the entire pipeline using the training data\n",
        "begin_knn_cy = time.perf_counter()\n",
        "pipe_knn_cy.fit(X_train_cy, y_train_cy)\n",
        "end_knn_cy = time.perf_counter()\n",
        "time_knn_cy = end_knn_cy - begin_knn_cy\n",
        "\n",
        "# Use the trained pipeline to make predictions on the test set\n",
        "y_pred_knn_cy = pipe_knn_cy.predict(X_test_cy)\n",
        "\n",
        "# Calculate the accuracy of the predictions\n",
        "accuracy_knn_cy = metrics.accuracy_score(y_test_cy, y_pred_knn_cy)"
