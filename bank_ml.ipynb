        "]\n",
        "\n",
        "# 2. Categorical variables (excluding 'contact', which was removed during preprocessing)\n",
        "# 'debt_score' is excluded here because it is treated as an ordinal variable.\n",
        "eng_categorical_cols = [\n",
        "    'job', 'marital', 'education', 'default', 'housing', 'loan',\n",
        "    'poutcome'\n",
        "]\n",
        "\n",
        "# 3. Ordinal variables (variables with an intrinsic ordering)\n",
        "eng_ordinal_cols = [\"month\", \"debt_score\"]\n",
        "\n",
        "# 4. pdays is kept separately for clarity, although it is numerical after cleaning\n",
        "eng_pdays = [\"pdays\"]\n",
        "\n"
