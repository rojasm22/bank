        "\n",
        "\n",
        "# -----------------------------------------------------------------------------\n",
        "# 1. Define Order for Ordinal Variables\n",
        "# -----------------------------------------------------------------------------\n",
        "# Manually define order to prevent alphabetical sorting\n",
        "month_order = ['jan','feb','mar','apr','may','jun','jul','aug','sep','oct','nov','dec']\n",
        "debt_order  = ['No debt', 'Low debt', 'Medium debt', 'High debt']\n",
        "\n",
        "# -----------------------------------------------------------------------------\n",
        "# 2. Define Transformers\n",
        "# -----------------------------------------------------------------------------\n",
        "\n",
        "# Numeric: Fill missing values with median and scale\n",
        "numerical_transformer_eng = Pipeline(steps=[\n",
        "    ('scaler', StandardScaler())\n",
        "])\n",
        "\n",
        "# Categorical: Fill missing with mode and One-Hot Encode (drop first to avoid multicollinearity)\n",
        "categorical_transformer_eng = Pipeline(steps=[\n",
        "    ('imputer', SimpleImputer(strategy='most_frequent')),\n",
        "    ('onehot', OneHotEncoder(handle_unknown='ignore', drop=\"first\"))\n",
        "])\n",
        "\n",
        "# Ordinal: Encode using the specific lists defined above\n",
        "ordinals_transformer_eng = Pipeline(steps=[\n",
        "    ('ordinal', OrdinalEncoder(categories=[month_order, debt_order]))\n",
        "])\n",
        "\n",
        "# Pdays Indicator: Create a boolean flag for 'never contacted' (value 999)\n",
        "pdays_indicator_transformer_eng = Pipeline(steps=[\n",
        "    ('indicator', MissingIndicator(missing_values=-1))\n",
        "])\n",
        "\n",
        "# -----------------------------------------------------------------------------\n",
        "# 3. Main Preprocessor\n",
        "# -----------------------------------------------------------------------------\n",
        "preprocessor_eng = ColumnTransformer(\n",
        "    transformers=[\n",
        "        # 1. Numeric features\n",
        "        ('num', numerical_transformer_eng, eng_numeric_cols),\n",
        "\n",
        "        # 2. Categorical features\n",
        "        ('cat', categorical_transformer_eng, eng_categorical_cols),\n",
        "\n",
        "        # 3. Ordinal features (Month, Debt Score)\n",
        "        ('ord', ordinals_transformer_eng, eng_ordinal_cols),\n",
        "\n",
        "        # 4. Pdays: Scale the numeric value (distance)\n",
        "        ('pdays_value', numerical_transformer_eng, eng_pdays),\n",
        "\n",
        "        # 5. Pdays: Add flag if never contacted (999)\n",
        "        ('pdays_contacted', pdays_indicator_transformer_eng, eng_pdays)\n",
        "    ],\n",
        "    remainder=\"drop\"\n",
        ")"
