        "def feature_engineering_leakage(df):\n",
        "    \"\"\"\n",
        "    Version of the Feature Engineering pipeline that KEEPS the variable 'duration'\n",
        "    to demonstrate Data Leakage effects in the predictive performance.\n",
        "    \"\"\"\n",
        "    bank_eng = df.copy()\n",
        "\n",
        "    # 1. Drop selected variables (we keep 'duration' on purpose for leakage analysis)\n",
        "    bank_eng = bank_eng.drop(['day', 'contact'], axis=1)\n",
        "\n",
        "\n",
        "    # 2. Debt Score: sum of three binary debt indicators\n",
        "    bank_eng['debt_score'] = (\n",
        "        bank_eng['housing'].apply(lambda x: 1 if x == 'yes' else 0) +\n",
        "        bank_eng['loan'].apply(lambda x: 1 if x == 'yes' else 0) +\n",
        "        bank_eng['default'].apply(lambda x: 1 if x == 'yes' else 0)\n",
        "    )\n",
        "\n",
        "    # Map debt score into ordinal text labels\n",
        "    label_dict = {0: 'No debt', 1: 'Low debt', 2: 'Medium debt', 3: 'High debt'}\n",
        "    bank_eng['debt_score'] = bank_eng['debt_score'].map(label_dict)\n",
        "\n",
        "    # 3. Wealth ratio: balance relative to age (a rough wealth proxy)\n",
        "    bank_eng['wealth_ratio'] = bank_eng['balance'] / bank_eng['age']\n",
        "\n",
        "    return bank_eng\n",
        "\n",
        "\n",
        "# A. Column lists for the comparative model\n",
        "# Add 'duration' to the existing numerical feature set to intentionally induce leakage\n",
        "leakage_numeric_cols = eng_numeric_cols + ['duration']\n",
        "\n",
        "# The remaining column lists are identical to the honest model\n",
        "leakage_categorical_cols = eng_categorical_cols\n",
        "leakage_ordinal_cols = eng_ordinal_cols\n",
        "leakage_pdays = eng_pdays\n",
        "\n",
        "\n",
        "# B. Comparative Preprocessor\n",
        "preprocessor_leakage = ColumnTransformer(\n",
        "    transformers=[\n",
        "        # Numerical transformer including the leaked variable 'duration'\n",
        "        ('num', numerical_transformer_eng, leakage_numeric_cols),\n",
        "\n",
        "        # Standard categorical pipeline\n",
        "        ('cat', categorical_transformer_eng, leakage_categorical_cols),\n",
        "\n",
        "        # Ordinal encoding for ordered variables\n",
        "        ('ord', ordinals_transformer_eng, leakage_ordinal_cols),\n",
        "\n",
        "        # pdays as a numerical feature\n",
        "        ('pdays_value', numerical_transformer_eng, leakage_pdays),\n",
        "\n",
        "        # Binary indicator for whether the client was ever contacted\n",
        "        ('pdays_contacted', pdays_indicator_transformer_eng, leakage_pdays)\n",
        "    ],\n",
        "    remainder=\"drop\"\n",
        ")\n",
        "\n",
        "# Define the model (same hyperparameters as the honest version to ensure fair comparison)\n",
        "clf_xgb_leakage = XGBClassifier(random_state=100576018)\n",
        "\n",
        "# Build the \"Evil Twin\" Pipeline including the leakage-producing feature engineering\n",
        "pipe_leakage = Pipeline([\n",
        "    ('feature_eng', FunctionTransformer(feature_engineering_leakage)),\n",
        "    ('preprocessor', preprocessor_leakage),\n",
        "    ('xgb', clf_xgb_leakage)\n",
        "])\n",
        "\n",
        "# Training\n",
        "begin_leak = time.perf_counter()\n",
        "pipe_leakage.fit(X_train, y_train)\n",
        "end_leak = time.perf_counter()\n",
        "\n",
        "# Prediction\n",
        "y_pred_xgb_leakage_eng = pipe_leakage.predict(X_test)\n"
