      "source": [
        "# Define the hyperparameter search space for Optuna optimization\n",
        "param_optuna_knn = {\n",
        "    # Number of neighbors (K) to consider, integer search space from 1 to 100\n",
        "    \"knn__n_neighbors\": IntDistribution(1, 100),\n",
        "\n",
        "    # How neighbors are weighted: 'distance' gives closer neighbors more influence, 'uniform' gives equal weight\n",
        "    \"knn__weights\": CategoricalDistribution([\"distance\", \"uniform\"]),\n",
        "\n",
        "    # Power parameter for Minkowski distance: 1 = Manhattan, 2 = Euclidean\n",
        "    \"knn__p\": IntDistribution(1, 2),\n",
        "}\n",
        "\n",
        "# Set the search budget (number of trials to run)\n",
        "budget = 30 ##30 attempts to make it faster\n",
        "\n",
        "# Create an Optuna-based hyperparameter search for the KNN pipeline\n",
        "knn_optuna = optuna_integration.OptunaSearchCV(\n",
        "    estimator=pipe_knn_ord,             # Pipeline containing preprocessing and KNN classifier\n",
        "    param_distributions=param_optuna_knn, # Search space for Optuna to explore\n",
        "    scoring='accuracy',                  # Metric to maximize during optimization\n",
        "    n_trials=budget,                     # Total number of trials to execute\n",
        "    cv=inner,                            # Cross-validation strategy\n",
        "    n_jobs=1,                            # Number of parallel jobs (1 = sequential execution)\n",
        "    verbose=1,                           # Display progress and trial results\n",
        "    timeout=600,                         # Maximum search time in seconds (10 minutes)\n",
        "    random_state=100576018,              # Seed for reproducibility\n",
        ")\n",
